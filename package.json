{
  "name": "link-shortener",
  "description": "Link Shortener is a Discord bot to easily shorten links!",
  "scripts": {
    "scripts:watch": "tsc --watch",
    "scripts:build": "tsc",
    "install:husky": "husky install",
    "install:develop": "npm i --production=false",
    "install:production": "npm i --production",
    "start": "node -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "bufferutil": "^4.0.3",
    "chalk": "^4.1.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "node-fetch": "^2.6.1",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "optionalDependencies": {
    "erlpack": "github:discord/erlpack",
    "zlib-sync": "^0.1.7"
  }
}
